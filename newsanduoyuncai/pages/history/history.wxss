page {
    display: block;
    min-height: 100%;
}
.circleBar {
    width: 100%;
    height: auto;
    overflow: hidden;
}

.list-block{
    width: 100%;
}

.list-block .item-inner {
    width: 100%;
    height: 50rpx;
