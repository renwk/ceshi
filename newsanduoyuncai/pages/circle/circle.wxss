}
.bottom-dialog-body{width:90%;margin:0 auto;position:absolute; z-index:10001; bottom:0; left:0; right:0;  height:100vh; background-color: #fff;}
/*动画前初始位置*/
.bottom-pos{-webkit-transform:translateY(100%);transform:translateY(100%);}
