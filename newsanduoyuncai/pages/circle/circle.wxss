page {display: block;min-height: 100%;
background-color: #F2F2F2;
}
.circleBar {
  width: 100%;
  height: auto;
  overflow: hidden;
  position: relative;
}

.wrap {
  width: 95%;
  margin: auto;

}
.main {
  width: 100%;
  border-radius: 30rpx;
  margin-top: 20rpx;
  background-color: #37bef0;
}
.top {
  text-align: center;
}
.timeandhistory {
  width: 90%;
  margin: auto;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  display: flex;
  flex-direction:row;
  justify-content:flex-end;
}
.timeandhistoryBox {
  color: #ffffff;
}
.times{
  width: 40%;
  font-size: 24rpx;
}
.historyTitle {
  width: 30%;
  font-size: 24rpx;
  text-align: right;
}
.cirleOut {
  display: flex;
  width: 162rpx;
  height: 162rpx;
  border: 2rpx solid #ffffff;
  border-radius: 81rpx;
  margin: 0 auto;
  text-align: center;
  vertical-align: middle;
}
.cir {
  display: inline-block;
  background-color: #37bef0;
  border-radius: 100%;
  margin: auto;

}
.centerWord {
  width: 100%;
  text-align: center;
  color: #FFFFFF;

}
.scoreTitle {
  position: absolute;
  top: 210rpx;
  left: 0;
  font-size: 20rpx;
}
.score {
  position: absolute;
  top: 156rpx;
  left: 0;
  font-size: 40rpx;
}
.moreThanBox {
  width: 100%;
  height: 60rpx;
  color: #ffffff;
  text-align: center;
  font-size: 22rpx;
  line-height: 60rpx;
}
.robotBox {
  width: 100%;
  height: 60rpx;
  text-align: center;
  padding-bottom: 20rpx;
}
.robotBtn {
  width: 180rpx;
  height: 50rpx;
  font-size: 20rpx;
  background-color: #fe5e0b;
  color: #ffffff;
  border-radius: 30rpx;

}
/*报告*/
.reportBox {
  width: 100%;
  margin-top: 20rpx;
  background-color: #ffffff;
  border-radius: 15rpx;
}
/*.reportDetailBox{
  width: 92%;
  margin: 0 auto;
}*/
.clearfix{zoom:1;}
.reportTitleBox {
  width: 90%;
  height:82.5rpx;
  margin: 0 auto;
  text-align: center;
  margin-top: 24rpx;

}
.reportTitle {
  width: 49.8%;
  color: #666666;
  font-size: 28.125rpx;
  font-weight:300;
  line-height: 82.5rpx;
  float: left;
}
