.container {
    height:100%;
    display:flex;
    flex-direction:column;
    align-items:center;
    justify-content:space-between;
    padding:200rpx 0;
    box-sizing:border-box
}
.nutrition_main .target_list {
    margin-top:20rpx
}
.nutrition_main {
    margin:30rpx auto;
    padding:0 26rpx
}
.user_info {
    position:relative;
    overflow:hidden;
    background:#37bef0;
    -webkit-border-radius:30rpx 30rpx;
    -moz-border-radius:30rpx 30rpx;
    border-radius:30rpx 30rpx
}
.user_info .header_img {
    float:left;
    margin:52rpx 0 52rpx 48rpx;
    width:144rpx;
    height:144rpx
}
.user_info .header_img image {
    display:block;
    width:100%;
    height:100%;
    border:2rpx solid #fff;
    -webkit-border-radius:50% 50%;
    -moz-border-radius:50% 50%;
    border-radius:50% 50%
}
.user_info .header_detail {
    float:left;
    margin-left:23rpx;
    color:#fff
}
.user_info .header_detail .h2 {
    margin-top:24rpx;
    line-height:44rpx
}
.user_info .header_detail .h2 text {
    font-size:36rpx
}
.user_info .header_detail .p {
    max-width:372rpx;
    font-size:24rpx
}
.user_info .history {
    position:absolute;
    right:24rpx;
    top:24rpx;
    width:140rpx;
    text-align:center;
    border:2rpx solid #fff;
    border-radius:100px;
    color:#fff;
    font-size:24rpx
}
@media screen and (max-width: 660rpx) {
    .user_info .header_detail {
        margin-left:26rpx
    }
    .user_info .header_img {
        margin:52rpx 0 52rpx 22rpx
    }
}
.cor-red {
    color:#f14e4e;
    border-color:#f14e4e
}
.common_foot {
    margin-top:30rpx;
    margin-bottom:30rpx;
    height:100rpx;
    line-height:100rpx;
    text-align:center;
    font-size:24rpx;
    color:#9e9e9e
}
.show_box {
    display:block
}
.hidden_box {
    display:none
}
.fl {
    float:left
}
.fr {
    float: right
}
/*圆形图标*/
.icon_round{
    position: relative;
    display: inline-block;
    width: 28rpx;
    height: 28rpx;
    background: #fff;
    vertical-align: middle;
    border: 4rpx solid #29c5e6;
    -webkit-border-radius:50% 50%;
    -moz-border-radius:50% 50%;
    border-radius:50% 50%;
}
.icon_round .b{
    position: absolute;
    top: 6rpx;
    left: 6rpx;
    display: block;
    width: 16rpx;
    height: 16rpx;
    background: #29c5e6;
    vertical-align: middle;
    -webkit-border-radius:50% 50%;
    -moz-border-radius:50% 50%;
    border-radius:50% 50%;
}
.nutrition_con{
    margin: 30rpx auto;
    padding: 30rpx 0;
    background: #fff;
    color: #484848;
    -webkit-border-radius:30rpx 30rpx;
    -moz-border-radius:30rpx 30rpx;
    border-radius:30rpx 30rpx;
}