page {
    background: #f9f9f9;
}
.body {
    padding: 24rpx;
}
.head_info {
    position: relative;
    overflow: hidden;
}
.head_info .user_pic {
    float: left;
    width: 176rpx;
    height: 176rpx;
    border-radius: 99rpx;
}
.head_info .info_main {
    margin-top: 30rpx;
    margin-left: 46rpx;
    float: left;
    font-size: 36rpx;
}
.con_box {
    margin-bottom: 24rpx;
    padding: 24rpx;
    width: 100%;
    box-sizing: border-box;
    border-radius: 20rpx;
    background: #fff;
    line-height:80rpx;
}

.me_list .row_title{
  width: 230rpx;
  margin-left:10rpx;
  display: inline-block;
}
.menu_list{
    overflow: hidden;
    width: 100%;
    padding-left: 16rpx;
    padding-right: 16rpx;
    padding-bottom: 0;
}
.menu_list .menu_one{
    float: left;
    display: block;
    width: 21%;
    margin: 0 2% 24rpx;
    padding: 20rpx 0 10rpx;
    text-align: center;
    color: #333;
    border-radius: 10rpx;
    box-shadow: 0px 6rpx 20rpx rgba(0,0,0,.1);
}
.menu_one text{
    font-size: 28rpx;
}
.s_icon{
    margin: 0 auto 12rpx;
    display: block;
    width: 56rpx;
    height: 56rpx;
}