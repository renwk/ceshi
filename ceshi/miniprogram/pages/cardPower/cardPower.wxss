page {
    background: #f9f9f9;
}
.body {
    padding: 24rpx;
}
.con_box {
    margin-bottom: 24rpx;
    padding: 24rpx;
    width: 100%;
    box-sizing: border-box;
    border-radius: 20rpx;
    background: #fff;
    line-height:80rpx;
}

.me_list .row_title{
  width: 230rpx;
  margin-left:10rpx;
  display: inline-block;
}
.menu_list{
    overflow: hidden;
    width: 100%;
    padding-left: 16rpx;
    padding-right: 16rpx;
    padding-bottom: 0;
}
.menu_list .menu_one{
    float: left;
    display: block;
    width: 21%;
    margin: 0 2% 24rpx;
    padding: 2r0px 0 10px;
    text-align: center;
    color: #333;
    border-radius: 10rpx;
    box-shadow: 0px 6rpx 20rpx rgba(0,0,0,.1);
}
.menu_one text{
    font-size: 28rpx;
}
